      "ember-cli-htmlbars-inline-precompile",
      "ember-cli-inject-live-reload",
      "ember-cli-qunit",
      "ember-cli-sri",
      "ember-cli-test-loader",
      "ember-resolver",
      "loader.js"
    ]
